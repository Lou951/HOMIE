

<div class="container">
  <%= simple_form_for [@list, @list_product] do |f| %>
    <%= f.input :frequency %>
    <%= f.association :product %>
    <%= f.button :submit%>
  <% end %>
</div>
<ul>
  <% @list_products.each do |product| %>
  <li>
    <%= product.product.item %>
    <%= product.frequency %>
    <% end  %>
  </li>
</ul>
